<h2>３ページ目</h2>

<%= form_for @debttable do |f| %>
  <%= render "devise/shared/error_messages", resource: @debttable %>

  <div class="field">
    <%= f.label "金額を入力" %><br />
    <%= f.text_field :balance %>
  </div>


  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>